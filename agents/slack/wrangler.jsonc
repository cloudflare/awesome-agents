{
  "name": "slack-agent",
  "main": "src/index.ts",
  "compatibility_date": "2025-03-14",
  "compatibility_flags": ["nodejs_compat"],
  "ai": {
    "binding": "AI"
  },
  "durable_objects": {
    "bindings": [
      {
        "class_name": "MyAgent",
        "name": "MyAgent"
      }
    ]
  },
  "migrations": [
    {
      "new_sqlite_classes": ["MyAgent"],
      "tag": "v1"
    }
  ],
  "observability": {
    "logs": {
      "enabled": true
    }
  }
}
